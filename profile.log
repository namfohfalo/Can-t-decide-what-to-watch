                                if cnt < 0
                                 let cnt= cnt + g:netrw_dirhistmax
                                endif
                               endwhile
                               exe "sil! w! ".savefile
                            "   call Decho("exe sil! w! ".savefile,'~'.expand("<slnum>"))
                              endif
                            
                              " save bookmarks
