                            "   "" call Decho("vim   directory: ".getcwd(),'~'.expand("<slnum>"))
                            "   "" call Decho("netrw directory: ".(exists("b:netrw_curdir")? b:netrw_curdir : 'n/a'),'~'.expand("<slnum>"))
                              endif
                            
                            "  "" call Dret("s:NetrwFile ".ret)
