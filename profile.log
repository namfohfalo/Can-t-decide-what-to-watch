                               " (comment from TM, solution from me)
                               let modkeep= &l:mod
                               exe "norm! o\<esc>"
                               call setline(line("."),a:x)
                               let ret= virtcol("$") - 1
                               d
                               NetrwKeepj norm! k
                               let &l:mod= modkeep
                            
                              else
                               " at least give a decent default
                                let ret= strlen(a:x)
                              endif
                            "  "" call Dret("s:Strlen ".ret)
