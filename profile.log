                            "   call Decho("2: dirname<".dirname.">",'~'.expand("<slnum>"))
                               let dirname= substitute(dirname,'\s\+$','','e')
                            "   call Decho("3: dirname<".dirname.">",'~'.expand("<slnum>"))
