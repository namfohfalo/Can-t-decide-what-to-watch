                               exe "sil! w! ".savefile
                            "   call Decho("exe sil! w! ".savefile,'~'.expand("<slnum>"))
                              endif
                            
                              " cleanup -- remove buffer used to construct history
                              let bgone= bufnr("%")
                              q!
                              exe "keepalt ".bgone."bwipe!"
                            
                            "  call Dret("s:NetrwBookHistSave")
                            endfun
                            
