                               let tgt    = a:1
                              endif
                            "  call Decho("tgt<".tgt.">",'~'.expand("<slnum>"))
                              let s:netrwmftgt         = tgt
                              let s:netrwmftgt_islocal = tgt !~ '^\a\{3,}://'
                              let curislocal           = b:netrw_curdir !~ '^\a\{3,}://'
