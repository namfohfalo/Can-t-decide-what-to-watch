                              if exists("s:netrwmarkfilelist")
                            "   "   call Decho("unlet'ing: s:netrwmarkfilelist",'~'.expand("<slnum>"))
                               unlet s:netrwmarkfilelist
                              endif
                            
                              let ibuf= 1
                              while ibuf < bufnr("$")
                               if exists("s:netrwmarkfilelist_".ibuf)
                                unlet s:netrwmarkfilelist_{ibuf}
